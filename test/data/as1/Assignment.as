//  Assignment expressions.

a = 1;
b = 2;
c = 3;
d = 4;
e = 5;

x = a;
x += b;
x -= b;
x *= d;
x /= b;
x %= e;
x <<= e;
x >>= a;
x >>>= b;
x &= c;
x |= d;
x ^= e;
x = a, x = b, x = c;
x = a = b = c;
