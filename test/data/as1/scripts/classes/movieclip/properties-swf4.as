//  MovieClip Properties.

onClipEvent(mouseDown)
{

_alpha = 50;
_height = 100;
_name = "newClip";
_rotation = 10;
_visible = true;
_width = 50;
_x = 100;
_xscale = 75;
_y = 100;
_yscale = 75;

this._alpha = 50;
this._height = 100;
this._name = "newClip";
this._rotation = 10;
this._visible = true;
this._width = 50;
this._x = 100;
this._xscale = 75;
this._y = 100;
this._yscale = 75;

clip._alpha = 50;
clip._height = 100;
clip._name = "newClip";
clip._rotation = 10;
clip._visible = true;
clip._width = 50;
clip._x = 100;
clip._xscale = 75;
clip._y = 100;
clip._yscale = 75;

clip._parent;
clip._parent._parent;

/*
 * Read-only movie clip properties 
 */
    
x = _currentframe;
x = _droptarget;
x = _framesloaded;
x = _target;
x = _totalframes;
x = _url;

x = this._currentframe;
x = this._droptarget;
x = this._framesloaded;
x = this._target;
x = this._totalframes;
x = this._url;

x = clip._currentframe;
x = clip._droptarget;
x = clip._framesloaded;
x = clip._target;
x = clip._totalframes;
x = clip._url;

}
