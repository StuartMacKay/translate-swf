/*
 * Create a Date object representing the current date and time.
 */
 
a = new Date();

/*
 * Check the get methods.
 */
 
a.getFullYear();
a.getMonth();
a.getDate();
a.getDay();
a.getHours();
a.getMinutes();
a.getSeconds();
a.getMilliseconds();
a.getTime();
a.getYear();
a.getTimezoneOffset();

/*
 * Check the set methods.
 */
 
a = new Date();

a.setFullYear(1900);
a.setMonth(0);
a.setDate(0);
a.setHours(0);
a.setMinutes(0);
a.setSeconds(0);
a.setMilliseconds(0);

a.setTime(1000);
a.setYear(1);
