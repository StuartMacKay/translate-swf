/* 
 * Floating-point Literals
 */

// signed numbers

-0.0;
 0.0;
+0.0;

// leading zeroes (more than 1 leading zero is an error)
 
 .0;
0.0;

// optional or signed exponents

1.0e;
1.0e-1;
1.0e1;
1.0e+1;

-1.0e;
-1.0e-1;
-1.0e1;
-1.0e+1;

+1.0e;
+1.0e-1;
+1.0e1;
+1.0e+1;

// large and small floating point numbers

2.225e-308;
1.798e308;
