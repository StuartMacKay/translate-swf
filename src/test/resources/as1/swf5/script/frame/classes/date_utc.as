/*
 * Create a Date object representing the current date and time.
 */
 
a = new Date();

/*
 * Check the UTC accessor methods.
 */
 
a.getUTCFullYear();
a.getUTCMonth();
a.getUTCDate();
a.getUTCDay();
a.getUTCHours();
a.getUTCMinutes();
a.getUTCSeconds();
a.getUTCMilliseconds();

/*
 * Check the UTC set methods.
 */

a.setUTCFullYear(1900);
a.setUTCMonth(0);
a.setUTCDate(0);
a.setUTCHours(0);
a.setUTCMinutes(0);
a.setUTCSeconds(0);
a.setUTCMilliseconds(0);

